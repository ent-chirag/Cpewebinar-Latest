<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rel_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@+id/relmain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_marginTop="@dimen/_10sdp">

        <ImageView
            android:id="@+id/ivwebinar_thumbhel"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_150sdp"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_5sdp"
            android:scaleType="fitXY" />

        <FrameLayout
            android:id="@+id/rel_bg_score"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_25sdp"
            android:layout_alignBottom="@+id/ivwebinar_thumbhel"
            android:visibility="gone">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/_25sdp"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/ivfavorite"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:layout_toLeftOf="@+id/tv_favorite_count"
                        android:padding="@dimen/_2sdp"
                        android:src="@drawable/like"
                        android:visibility="gone" />

                    <com.myCPE.view.CustomTextView
                        android:id="@+id/tv_favorite_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/_10sdp"
                        android:textColor="@color/White"
                        android:textSize="@dimen/_12sdp"
                        android:visibility="gone"
                        app:font_text="Montserrat-Medium.ttf" />

                    <ImageView
                        android:id="@+id/ivgroup"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/_10sdp"
                        android:layout_toLeftOf="@+id/tv_attend_views"
                        android:src="@mipmap/group"
                        android:visibility="gone" />

                    <com.myCPE.view.CustomTextView
                        android:id="@+id/tv_attend_views"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/_2sdp"
                        android:textColor="@color/White"
                        android:textSize="@dimen/_12sdp"
                        android:visibility="gone"
                        app:font_text="Montserrat-Medium.ttf" />

                </RelativeLayout>

            </RelativeLayout>

        </FrameLayout>

        <RelativeLayout
            android:id="@+id/relbottom"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ivwebinar_thumbhel"
            android:layout_alignParentBottom="true"
            android:layout_marginTop="@dimen/_5sdp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/ivmain"
                android:visibility="gone">

            </RelativeLayout>

            <!--  <ImageView
                  android:id="@+id/ivmain"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_centerInParent="true"
                  android:background="@mipmap/webinar_vertical_divider" />-->

            <View
                android:id="@+id/ivmain"
                android:layout_width="1dp"
                android:layout_height="70dp"
                android:layout_centerHorizontal="true"
                android:background="@color/fav_date_time"
                android:visibility="gone"></View>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_5sdp"
                android:layout_toRightOf="@+id/ivmain">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="3">

                        <com.myCPE.view.CustomButtonView
                            android:id="@+id/tv_webinar_price_status"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_25sdp"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:background="@drawable/rounded_webinar_status"
                            android:textColor="@color/White"
                            android:textSize="@dimen/_9sdp"
                            app:font_button="Montserrat-Medium.ttf"/>

                        <com.myCPE.view.CustomButtonView
                            android:id="@+id/credit_status"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_25sdp"
                            android:layout_weight="1"
                            android:textColor="@color/White"
                            android:background="@drawable/rounded_webinar_status"
                            android:textSize="@dimen/_9sdp"
                            app:font_button="Montserrat-Medium.ttf" />

                        <com.myCPE.view.CustomButtonView
                            android:id="@+id/tv_webinar_type"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_25sdp"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:background="@drawable/rounded_webinar_status"
                            android:textColor="@color/White"
                            android:textSize="@dimen/_9sdp"
                            app:font_button="Montserrat-Medium.ttf" />

                    </LinearLayout>

                    <RelativeLayout
                        android:id="@+id/rel_speaker_details"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp">

                        <ImageView
                            android:id="@+id/iv_speaker"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:src="@mipmap/home_webinar_user" />

                        <com.myCPE.view.CustomTextView
                            android:id="@+id/tv_speaker_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_toLeftOf="@+id/tv_rating_number"
                            android:layout_toRightOf="@+id/iv_speaker"
                            android:inputType="textNoSuggestions"
                            android:singleLine="true"
                            android:textColor="@color/home_tab_color_unselected"
                            android:textSize="@dimen/_10sdp"
                            app:font_text="OpenSans-Regular.ttf" />

                        <com.myCPE.view.CustomTextView
                            android:id="@+id/tv_rating_number"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_toLeftOf="@+id/iv_rating"
                            android:textColor="@color/webinar_status"
                            android:textSize="@dimen/_8sdp"
                            app:font_text="Montserrat-Bold.ttf" />

                        <ImageView
                            android:id="@+id/iv_rating"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_toLeftOf="@+id/tv_rating_count" />

                        <com.myCPE.view.CustomTextView
                            android:id="@+id/tv_rating_count"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:textColor="@color/home_tab_color_unselected"
                            android:textSize="@dimen/_8sdp"
                            app:font_text="Montserrat-Bold.ttf" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/rel_company_details"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp">

                        <ImageView
                            android:id="@+id/iv_company"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:src="@mipmap/home_company" />

                        <com.myCPE.view.CustomTextView
                            android:id="@+id/tv_company_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_toRightOf="@+id/iv_company"
                            android:inputType="textMultiLine|textNoSuggestions"
                            android:textColor="@color/home_tab_color_unselected"
                            android:textSize="@dimen/_10sdp"
                            app:font_text="OpenSans-Regular.ttf" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/rel_date_and_time"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp">

                        <ImageView
                            android:id="@+id/iv_clock"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:src="@mipmap/home_clock" />

                        <com.myCPE.view.CustomTextView
                            android:id="@+id/tv_webinar_date"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_toRightOf="@+id/iv_clock"
                            android:inputType="textNoSuggestions"
                            android:textColor="@color/webinar_status"
                            android:textSize="@dimen/_10sdp"
                            app:font_text="OpenSans-Regular.ttf" />

                        <View
                            android:id="@+id/dv_divider"
                            android:layout_width="1dp"
                            android:layout_height="@dimen/_10sdp"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_marginTop="@dimen/_7sdp"
                            android:layout_toRightOf="@+id/tv_webinar_date"
                            android:background="@color/webinar_status" />

                        <com.myCPE.view.CustomTextView
                            android:id="@+id/tv_webinar_time"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_toRightOf="@+id/dv_divider"
                            android:textColor="@color/webinar_status"
                            android:textSize="@dimen/_10sdp"
                            app:font_text="OpenSans-Regular.ttf" />

                        <com.myCPE.view.CustomTextView
                            android:id="@+id/tv_timezone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_toRightOf="@+id/tv_webinar_time"
                            android:textColor="@color/webinar_status"
                            android:textSize="@dimen/_10sdp"
                            app:font_text="OpenSans-Regular.ttf" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp">

                        <com.myCPE.view.CustomTextView
                            android:id="@+id/tv_lable"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"
                            android:ellipsize="end"
                            android:ems="11"
                            android:inputType="textMultiLine|textNoSuggestions"
                            android:textAllCaps="true"
                            android:textColor="@color/red_warning"
                            android:textSize="@dimen/_9sdp"
                            app:font_text="Montserrat-SemiBold.ttf" />

                        <com.myCPE.view.CustomTextView
                            android:id="@+id/tv_enrolled"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:textColor="@color/home_tab_color_unselected"
                            android:textSize="@dimen/_10sdp"
                            app:font_text="Montserrat-Bold.ttf" />

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/relBottomButton"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_5sdp">

                        <com.myCPE.view.CustomButtonView
                            android:id="@+id/webinar_status"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_25sdp"
                            android:layout_marginRight="@dimen/_35sdp"
                            android:background="@drawable/rounded_webinar_home"
                            android:textColor="@color/White"
                            android:textSize="@dimen/_9sdp"
                            app:font_button="Montserrat-Medium.ttf" />

                        <ImageView
                            android:id="@+id/ivshare"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:padding="@dimen/_5sdp"
                            android:layout_marginRight="@dimen/_10sdp"
                            android:layout_toLeftOf="@+id/webinar_status"
                            android:src="@mipmap/home_share"/>

                    </RelativeLayout>

                </LinearLayout>

            </RelativeLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rv_invisible"
            android:layout_width="match_parent"
            android:layout_height="5dp"
            android:layout_below="@+id/relbottom"
            android:visibility="invisible">

        </RelativeLayout>

    </RelativeLayout>

</RelativeLayout>