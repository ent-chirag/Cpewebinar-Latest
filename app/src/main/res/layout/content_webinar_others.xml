<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_marginRight="@dimen/_5sdp"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp">

            <RelativeLayout
                android:id="@+id/rel_faq"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5sdp"
                android:padding="@dimen/_5sdp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:gravity="center"
                    android:text="@string/str_faq"
                    android:textColor="@color/color_text_black"
                    android:textSize="@dimen/_14sdp"
                    android:fontFamily="@font/whitney_semibold" />

            </RelativeLayout>

            <TextView
                android:id="@+id/tv_faq"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rel_faq"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:textColor="@color/color_text_black_80"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/whitney_book" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:padding="@dimen/_5sdp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:gravity="center"
                android:text="@string/str_refund_cancelation_policy"
                android:textColor="@color/color_text_black"
                android:textSize="@dimen/_14sdp"
                android:fontFamily="@font/whitney_semibold" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_5sdp">

            <TextView
                android:id="@+id/tv_refund_cancelation_policy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:textColor="@color/color_text_black_80"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/whitney_book" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_nasba"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:padding="@dimen/_5sdp"
            android:visibility="gone">

            <TextView
                android:id="@+id/str_nasbaapproved"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:text="@string/str_nasba_approved"
                android:textColor="@color/color_text_black"
                android:textSize="@dimen/_14sdp"
                android:fontFamily="@font/whitney_semibold" />

            <TextView
                android:id="@+id/txt_nasba_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/str_nasbaapproved"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/linNasbaImage"
                android:textColor="@color/color_text_black"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/whitney_medium" />

            <LinearLayout
                android:id="@+id/linNasbaImage"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/_25sdp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/_5sdp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/iv_nasba_profile"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_25sdp" />

                <ImageView
                    android:id="@+id/iv_nasba_profile_qas"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_25sdp"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:visibility="gone" />

            </LinearLayout>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_nasba_desc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            android:visibility="gone">

            <TextView
                android:id="@+id/txt_nasba_description"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:textColor="@color/color_text_black_80"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/whitney_book" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_ea"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:padding="@dimen/_5sdp"
            android:visibility="visible">

            <TextView
                android:id="@+id/str_ea_approved"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:text="@string/str_ea_approved"
                android:textColor="@color/color_text_black"
                android:textSize="@dimen/_14sdp"
                android:fontFamily="@font/whitney_semibold" />

            <TextView
                android:id="@+id/txt_ea_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/str_ea_approved"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/iv_ea_profile"
                android:textColor="@color/color_text_black_80"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/whitney_medium" />

            <ImageView
                android:id="@+id/iv_ea_profile"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_25sdp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/_5sdp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_ea_desc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            android:visibility="visible">

            <TextView
                android:id="@+id/txt_ea_description"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:textColor="@color/color_text_black_80"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/whitney_book" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_irs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:padding="@dimen/_5sdp"
            android:visibility="visible">

            <TextView
                android:id="@+id/str_irs_approved"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:text="@string/str_irs_approved"
                android:textColor="@color/color_text_black"
                android:textSize="@dimen/_14sdp"
                android:fontFamily="@font/whitney_semibold" />

            <TextView
                android:id="@+id/txt_irs_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/str_irs_approved"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/iv_irs_profile"
                android:textColor="@color/color_text_black_80"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/whitney_medium" />

            <ImageView
                android:id="@+id/iv_irs_profile"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_25sdp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/_5sdp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_irs_desc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            android:visibility="visible">

            <TextView
                android:id="@+id/txt_irs_description"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:textColor="@color/color_text_black_80"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/whitney_book" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_ctec"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:padding="@dimen/_5sdp"
            android:visibility="gone">

            <TextView
                android:id="@+id/str_ctecapproved"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:text="@string/str_ctec_approved"
                android:textColor="@color/color_text_black"
                android:textSize="@dimen/_14sdp"
                android:fontFamily="@font/whitney_semibold" />

            <TextView
                android:id="@+id/txt_ctec_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/str_ctecapproved"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_toLeftOf="@+id/iv_ctec_profile"
                android:textColor="@color/color_text_black_80"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/whitney_medium" />

            <ImageView
                android:id="@+id/iv_ctec_profile"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_25sdp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/_5sdp" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rel_ctec_desc"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginBottom="@dimen/_5sdp"
            android:visibility="gone">

            <TextView
                android:id="@+id/txt_ctec_description"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:textColor="@color/color_text_black_80"
                android:textSize="@dimen/_12sdp"
                android:fontFamily="@font/whitney_book" />

        </RelativeLayout>

    </LinearLayout>

</LinearLayout>
